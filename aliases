#!/bin/bash
alias dr="docker-compose run laravel"
alias php="dr php"
alias artisan="php artisan"
alias __do__test="artisan test"
alias tst="artisan config:cache && alias tst=__do__test && __do__test"
alias up="docker-compose down && docker-compose up"
alias 0db="sudo rm laravel/database/database.sqlite && touch laravel/database/database.sqlite && artisan migrate && up"